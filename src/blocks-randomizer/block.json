{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blocks-randomizer/holder",
	"version": "1.3.1",
	"title": "Blocks Randomizer",
	"category": "widgets",
	"icon": "randomize",
	"description": "Display randomly any of the top-level blocks within the \"Blocks Randomizer\" main block.",
	"attributes": {
		"numberOfItems": {
			"type": "number",
			"default": 1
		},
		"shuffle": {
			"type": "boolean",
			"default": false
		},
		"preventRepeatsUsingSession": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "blocks-randomizer",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
